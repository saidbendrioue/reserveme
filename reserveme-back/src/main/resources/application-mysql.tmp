spring:
  datasource:
    url: jdbc:mysql://localhost:3306/reserve_me_db
    username: root
    password:
    driver-class-name: com.mysql.cj.jdbc.Driver
  sql:
    init:
      data-locations: classpath:import.sql
  jpa:
    hibernate:
      ddl-auto: create-drop
    show-sql: true
logging:
  level:
    org:
      springframework:
        security: DEBUG
image:
  upload:
    directory: C:\reserveme\images\
  cdn:
    url: http://localhost:8080/api/files